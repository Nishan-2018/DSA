class Solution {
public:
    int secondLargestElement(vector<int>& nums) {
        //your code goes here
        sort(nums.begin(),nums.end());
        int n=nums.size();
        int max1=nums[n-1];
        int max2=INT_MIN;
        for(int i=n-2;i>=0;i--)
        {
            if(nums[i]<max1)
            {
                max2=nums[i];
                break;
            }
        }
        if(max2==INT_MIN)
        {
            return-1;
        }
        return max2;

      
    }
};
